{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Host=__HOST__;Database=__DB__;Username=__USER__;Password=__PASS__;"
    },
    "TokenSettings": {
        "Key": "__YOUR_SECRET_KEY__",
        "Issuer": "Sufra-BackEnd",
        "Audience": "*",
        "AccessTokenExpirationMinutes": 30,
        "RefreshTokenExpirationDays": 7
    },
    "SmtpSettings": {
        "Host": "__YOUR_HOST__ex:smtp.gmail.com",
        "Port": 587,
        "Username": "YOUR_EMAIL",
        "Password": "YOUR_APP_PASSWORD",
        "FromName": "Sufra",
        "FromEmail": "YOUR_EMAIL"
    },
    "SupportSettings": {
        "Email": "__YOUR_SUPPORT_MAIL",
        "Phone": "__YOUR_SUPPORT_PHONE"
    }

}
